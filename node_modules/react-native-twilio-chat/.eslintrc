{
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "extends": [
      "airbnb"
    ],
    "settings": {
      "import/core-modules": ["react-native"],
      "no-unused-vars": ["error", { "varsIgnorePattern": "React" }]
    },
    "rules": {
      "import/extensions": ["off", "never"],
      "no-underscore-dangle": 0
    }
}